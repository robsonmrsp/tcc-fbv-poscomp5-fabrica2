<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="sisa.com.br.entidade.Financiamento" table="financiamento">

		<id name="qtdeParcelas" column="qtd_parcelas" type="java.lang.Integer" unsaved-value="0">
			<generator class="assigned"/>
		</id>
		<property column="perc_financiamento" type="java.lang.Double" name="percentual" not-null="true" />
		
	</class>
</hibernate-mapping>
